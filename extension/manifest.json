{
  "manifest_version": 3,
  "name": "CAID Plugin",
  "version": "0.1.0",
  "description": "A plugin to identify and tag deepfake images.",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "blockhash.js",
        "content.js"
      ]
    }
  ],
  "icons": {
    "16": "images/Lcaid.png",
    "48": "images/Lcaid.png",
    "128": "images/Lcaid.png"
  }
}